{
  "configVersion": "v3",
  "app": {
    "appId": 28964,
    "appName": "WatchDrip+",
    "appType": "app",
    "version": {
      "code": 1,
      "name": "2.0.0"
    },
    "vender": "bigdigital",
    "description": "Watchdrip application, required for wathcfaces",
    "icon": "icon.png"
  },
  "permissions": [
    "data:os.device.info",
    "device:os.alarm",
    "device:os.notification",
    "device:os.ble",
    "device:os.local_storage",
    "device:os.bg_service",
    "event:customize.fetch"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "3.0.0",
      "target": "3.0.0",
      "minVersion": "3.0"
    }
  },
  "targets": {
    "common": {
      "module": {
        "page": {
          "pages": ["page/index"]
        },
        "app-service": {
          "services": ["app-service/fetch_service"]
        },
        "app-side": {
          "path": "app-side/index"
        },
        "app-event": {
          "path": "app-service/fetch_service",
          "path": "app-service/alarm_service"
        }
      },
      "platforms": [
        {
          "st": "r",
          "dw": 480
        }
      ]
    }
  },
  "i18n": {
    "en-US": {
      "appName": "WatchDrip+"
    }
  },
  "defaultLanguage": "en-US",
  "debug": true
}
